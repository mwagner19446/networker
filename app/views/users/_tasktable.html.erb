<table >
  <tr>
    <th id="task_delete"></th>
    <th id="task_type">Type</th>
    <th id="task_title">Title</th>
    <th id="task_notes">Notes</th>
    <th id="task_due">Due Date</th>
  </tr>
  <% @tasks.each do |task| %>
  <tr>
    <td><%=link_to image_tag("delete.png",size: "20x20"), user_task_path(params[:id],task.id), method: "delete" %></td>
    <td><%=task.t_type%></td>
    <td><%=link_to task.title, edit_user_task_path(params[:id], task.id)%></td>
    <td><%=task.notes%></td>
    <td><%=task.due_date.strftime("%Y-%m-%d")%></td>
  </tr> 
  <% end %>
</table>