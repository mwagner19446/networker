<h1>Upcoming Meetups</h1>
Find Meetups Groups that match your interest

<%= render "form" %>


<%unless @from_meetup.nil? %>
  <table >
    <tr>
      <th>Photo</th>
      <th>Name</th>
      <th>Description</th>
      <th><th>
    </tr>
    <% @from_meetup.each do |meetup| %>
    <tr>
      <td></td>
      <td><img src="<%=meetup["photo_url"]%>" height="80" width="80" ></td>
      <td><%=meetup["who"]%></td>
      <td><%=meetup["description"]%></td>
      <td><%= link_to "Add", user_tasks_path(params[:id], :title=>("Meetup: "+meetup["group"]["name"]) ), method: "post" %></td>
    </tr> 
    <% end %>
  </table>
<%end%>